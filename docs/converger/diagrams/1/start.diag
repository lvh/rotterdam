seqdiag {
        autonumber = True;
        default_fontsize = 16;

        planner [label="Planner"];
        converger [label="Converger"];
        nova [label="Nova"];
        clb [label="CLB"];

        planner ->> converger [label="Group A: 5 machines!"];

        converger => clb [label="machines in A?", return="{a, b, c}"];
        converger => nova [label="machines for tenant of A?", return="{a, b, c}"];
        converger => nova [label="+2 machines in A!", return="{d, e}"];
}
